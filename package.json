{
  "private": true,
  "name": "thedailydietitian",
  "version": "1.0.0",
  "description": "The Daily Dietitian",
  "repository": "github:thedailydietitian/thedailydietitian",
  "main": "",
  "scripts": {
    "pretest": "npm run build",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:e2e": "start-server-and-test start http://localhost:8080 cypress",
    "test:e2e:update": "start-server-and-test start http://localhost:8080 cypress:update",
    "test:e2e:open": "start-server-and-test start http://localhost:8080 cypress:open",
    "cypress": "cypress run",
    "cypress:update": "cypress run --env updateSnapshots=true",
    "cypress:open": "cypress open",
    "start": "eleventy --serve",
    "prebuild": "trash public",
    "build": "eleventy"
  },
  "author": "Simon Sinclair",
  "license": "UNLICENSED",
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "axe-core": "^3.3.2",
    "clean-css": "^4.2.1",
    "cypress": "^3.4.1",
    "cypress-axe": "^0.5.1",
    "cypress-image-snapshot": "^3.1.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "html-minifier": "^4.0.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "start-server-and-test": "^1.9.2",
    "terser": "^4.3.1",
    "trash-cli": "^3.0.0"
  }
}
